    "@types/express": "^4.17.3",
    "@types/express-promise-router": "^3.0.0",
    "@types/pg": "^7.14.3",
    "node-loader": "^0.6.0",
    "null-loader": "^3.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
